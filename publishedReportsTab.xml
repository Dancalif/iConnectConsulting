<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="PublishedReports" thread-count="4" parallel="tests">
    <test name="Chrome">
        <parameter name="conf" value="conf"/>
        <parameter name="environment" value="chrome"/>
        <classes>
            <class name="iConnectConsulting.PublishedReportsTab"/>
        </classes>
    </test>

    <test name="FireFox">
        <parameter name="conf" value="conf"/>
        <parameter name="environment" value="firefox"/>
        <classes>
            <class name="iConnectConsulting.PublishedReportsTab"/>
        </classes>
    </test>

    <test name="Safari">
        <parameter name="conf" value="conf"/>
        <parameter name="environment" value="safari"/>
        <classes>
            <class name="iConnectConsulting.PublishedReportsTab"/>
        </classes>
    </test>

    <test name="IE">
        <parameter name="conf" value="conf"/>
        <parameter name="environment" value="ie"/>
        <classes>
            <class name="iConnectConsulting.PublishedReportsTab"/>
        </classes>
    </test>
</suite>